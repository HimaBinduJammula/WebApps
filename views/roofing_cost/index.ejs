<% console.log(estimatePartRoofings.query); %>
<div class="container">
 <h2>All entries</h2>

<p>There are  <%= estimatePartRoofings.query.length %> entries. <br/>
<a href="estimatePartRoofing/create"><i class = "glyphicon glyphicon-pencil"></i> Add a new one.</a></p>

<table class="table" border="1">
    <thead>
      <tr>
        <th>ID</th>
        <th>Roof Type</th>
        <th>Process Type</th>
        <th>Total Cost</th>
        <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    <% var subTotal = 0 %>
<% for (var i = 0; i < estimatePartRoofings.query.length; i++) { 
    subTotal += estimatePartRoofings.query[i].subtotal;
    %>
         <tr>
            <td><%=  estimatePartRoofings.query[i]._id %></td>
            <td><%=  estimatePartRoofings.query[i].roofType %></td>
            <td><%=  estimatePartRoofings.query[i].processType %></td>
            <td> $<%=  estimatePartRoofings.query[i].subtotal %></td>
            <td><a class = "btn btn-danger" href="estimatePartRoofing/delete/<%= estimatePartRoofings.query[i]._id %>"><i class = "glyphicon glyphicon-trash"></i> Delete</a>
                <a class = "btn btn-info" href="estimatePartRoofing/details/<%= estimatePartRoofings.query[i]._id %>"><i class = "glyphicon glyphicon-list-alt"></i> Details</a>
                <a class = "btn btn-warning" href="estimatePartRoofing/edit/<%= estimatePartRoofings.query[i]._id %>"><i class = "glyphicon glyphicon-edit"></i> Edit</a></td>
         </tr>
         <% } %>
</tbody>
</table>
<p> Total Estimated Cost for Roofing : <strong> $<%= subTotal %> </strong> </p>
</div>
