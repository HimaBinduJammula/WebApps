
<% console.log(roofingTopcoats.query); %>
<div class="container">
 <h2>Material roofing topcoat costs</h2>

<p>There are  <%= roofingTopcoats.query.length %> roofing topcoats. <br/>
<a href="roofingTopcoat/create" class="btn btn-primary"><span class="glyphicon glyphicon-pencil"></span>Add a new one.</a></p>

<table class="table" border="1">
    <thead>
      <tr>
      <th>ID</th>
        <th>Name</th>
        <th>Display Order</th>
        <th>Price</th>
        <th>Unit</th>
       <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    <% count=0; %>
<% for (var i = 0; i < roofingTopcoats.query.length; i++) { %>
         <tr><% count++; %>
         <td><%=  roofingTopcoats.query[i]._id %></td>
            <td><%=  roofingTopcoats.query[i].name %></td>
            <td><%=  roofingTopcoats.query[i].displayorder %></td>
            <td><%=  roofingTopcoats.query[i].price %></td>
            <td><%=  roofingTopcoats.query[i].unit %></td>
            <td>
							<a href="roofingTopcoat/delete/<%= roofingTopcoats.query[i]._id %>" class="btn btn-danger" >	<span class="glyphicon glyphicon-trash"></span> Delete </a>
							<a href="roofingTopcoat/details/<%=  roofingTopcoats.query[i]._id %>" class="btn btn-info">	  <span class="glyphicon glyphicon-list-alt"></span> Details </a>
							<a href="roofingTopcoat/edit/<%=  roofingTopcoats.query[i]._id %>" class="btn btn-warning"> <span class="glyphicon glyphicon-edit"></span> Edit </a>
						</td>
         </tr>
         <% } %>
</tbody>
</table>
</div>
