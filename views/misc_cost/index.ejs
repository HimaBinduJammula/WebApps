
<% console.log(estimatePartMiscs.query); %>
<div class="container">
 <h2>All entries</h2>

<p>There are  <%= estimatePartMiscs.query.length %> entries. <br/>
<a href="estimatePartMisc/create">Add a new one.</a></p>

<table class="table" border="1">
    <thead>
      <tr>
       <th>ID</th>
       <th>Description</th>
       <th>Cost</th>
       <th>Actions</th>
    </tr>
    </thead>
    <tbody>
<% for (var i = 0; i < estimatePartMiscs.query.length; i++) { 
    var totalCost = 0;
     for(var j=0; j<estimatePartMiscs.query[i].entries.length;j++){
        var entry = estimatePartMiscs.query[i].entries[j];
      %>
        <tr>
            <td> <%= entry._id %></td>
            <td> <%= entry.description %></td>
            <td> <%= entry.cost %></td>
            <% totalCost += entry.cost %>
            <td><a href="estimatePartMisc/delete/<%= entry._id %>"> Delete</a> | <a href="estimatePartMisc/details/<%= entry._id %>"> Details</a> | <a href="estimatePartMisc/edit/<%= entry._id %>"> Edit</a></td>

        </tr>
    <%  } %>
         <tr>
            <td colspan="4" align="center"> <strong> Totla Estimated Miscellaneous Cost :  <span style="color:green"><%=  totalCost %> </strong></span> </tbody></td>
         </tr>
         <% } %>
</tbody>
</table>
</div>

