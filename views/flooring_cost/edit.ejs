<h2>Edit estimatePartFlooring product</h2>
<div class="content">
	<div>
		<form method="POST" action="/estimatePartFlooring/save/<%= estimatePartFlooring._id %>">

			<div class="form-group" id=frm>
				<label for="_id">ID</label>
				<input type="number" class="form-control" id="_id" name="_id" value="<%= estimatePartFlooring._id %>" required disabled>
			</div>

			<div class="form-group">
				<label for="FloorSystemType">FloorSystemType</label>
				<input type="text" class="form-control" id="floorSystemType" name="floorSystemType" value="<%= estimatePartFlooring.roofType %>" required>
			</div>

			<div class="form-group">
				<label for="processType">processType</label>
				<input type="text" class="form-control" id="usesUrethane" name="usesUrethane" value="<%= estimatePartFlooring.processType %>" required>
			</div>

			<div class="form-group">
				<label for="price">subtotal</label>
				<input type="number" class="form-control" id="subtotal" name="subtotal" value="<%= estimatePartFlooring.subtotal %>" required>
			</div>

			<input type="submit" value="Save" class="btn btn-primary" />
		</form>
		<br/>
		<form method="get" action="/estimatePartFlooring">
			<input type="submit" value="Return to list" class="btn btn-caution" />
		</form>
	</div>
</div>
