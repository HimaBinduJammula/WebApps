<h2>Edit Flooring Estemate</h2>
<div class="content">
	<div>
		<form method="POST" action="/flooringEstimate/save/<%= flooringEstimate._id %>">

			<div class="form-group" id=frm>
				<label for="_id">ID</label>
				<input type="number" class="form-control" id="_id" name="_id" value="<%= flooringEstimate._id %>" required disabled>
			</div>

			<div class="form-group">
				<label for="Client">Client</label>
				<input type="text" class="form-control" id="Client" name="Client" value="<%= flooringEstimate.estimatePartAbout.client %>" required>
			</div>

		    <div class="form-group">
				<label for="Created">Created</label>
				<input type="Date" class="form-control" id="Created" name="Created" value="<%=flooringEstimate.estimatePartAbout.created%>"
					required>
			</div>
            <div class="form-group">
				<label for="Address">Address</label>
				<input type="text" class="form-control" id="Address" name="Address" value="<%=flooringEstimate.estimatePartAbout.address%>"
					required>
			</div>

            <div class="form-group">
				<label for="City">City</label>
				<input type="text" class="form-control" id="City" name="City" value="<%=flooringEstimate.estimatePartAbout.city%>"
					required>
			</div>

            <div class="form-group">
				<label for="State">State</label>
				<input type="text" class="form-control" id="State" name="State" value="<%=flooringEstimate.estimatePartAbout.state%>"
					required>
			</div>

            <div class="form-group">
				<label for="zipcode">zipcode</label>
				<input type="number" class="form-control" id="zipcode" name="zipcode" value="<%=flooringEstimate.estimatePartAbout.zipcode%>"
					required>
			</div>
			
			<div class="form-group">
				<label for="latitude">latitude</label>
				<input type="number" class="form-control" id="latitude" name="latitude" value="<%=flooringEstimate.estimatePartAbout.latitude%>"
					required>
			</div>

            <div class="form-group">
				<label for="Longitude">Longitude</label>
				<input type="number" class="form-control" id="Longitude" name="Longitude" value="<%=flooringEstimate.estimatePartAbout.Longitude%>"
					required>
			</div>


            <div class="form-group">
				<label for="Squre_Footage">Squre Footage</label>
				<input type="number" max=20000 min=400 class="form-control" id="Squre_Footage" name="Squre_Footage" value="<%=flooringEstimate.estimateSquareFootage.sqft%>"
					required>
			</div>

            <div class="form-group">
				<label for="CostPerSquareFoot">CostPerSquareFoot</label>
				<input type="number" step = 0.01 class="form-control" id="CostPerSquareFoot" name="CostPerSquareFoot" value="<%=flooringEstimate.costPerSquareFoot%>"
					required>
			</div>

            <div class="form-group">
				<label for="BidPerSquareFoot">BidPerSquareFoot</label>
				<input type="number" step = 0.01 class="form-control" id="BidPerSquareFoot" name="BidPerSquareFoot" value="<%=flooringEstimate.bidPerSquareFoot%>"
					required>
			</div>

			<input type="submit" value="Save" class="btn btn-primary" />
		</form>
		<br/>
		<form method="get" action="/flooringEstimate">
			<input type="submit" value="Return to list" class="btn btn-caution" />
		</form>
	</div>
</div>

